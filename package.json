{
  "name": "draganddrop",
  "version": "1.0.0",
  "description": "Run The Application ```shell npx serve . ```",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel watch js:watch main:watch serve",
    "watch": "npx tailwindcss -i src/tailwind.css -o dist/utilities.css --watch",
    "js:watch": "esbuild src/icons.js --bundle --minify --outfile=dist/icons.js --watch",
    "main:watch": "esbuild src/main.js --bundle --minify --outfile=dist/main.js --watch",
    "serve": "live-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roycanales17/Web-Builder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roycanales17/Web-Builder/issues"
  },
  "homepage": "https://github.com/roycanales17/Web-Builder#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@tailwindcss/cli": "^4.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.8",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  }
}
